* {
	font-size: small;
	font-family: Verdana, Arial;
}

.light1 {
	background: #eef0f7;
}
.light2 {
	background: #d1dceb;
}

.dvBlue {
	background: #5c74a9;
}

.dvBlueWhite {
	background: #5c74a9;
	color: white;
}

.grey {
	background: #efefef;
}

.darkGrey {
	background: #E8E8E8;	
}

.bold {
	font-weight: bold;
}

.infoMsg {
	color: #f00;
	font-weight: bold;
}

.warningMsg {
    color: #ff4500;
	font-weight: bold;
}

table.standardPanel {
	border: 1px solid #5c74a9;
	border-collapse: collapse;
}

table.standardPanel th.panelHeader {
	background-image: url(/images/blueTableHeader.gif); 
	background-repeat: repeat-x;
	color: white;
	font-weight: bold;
	text-align: left;
	padding: 5px 5px 5px 5px;
}

table.standardPanel td.panelContent {
	padding: 10px 20px 10px 20px;
}

table.kkeCreditTable td {
	padding: 2px 5px 2px 5px;
	border-bottom: 1px solid #5c74a9;
}

.button1 {
	border: 1px solid #999999;
	border-top-color: #CCCCCC;
	border-left-color: #CCCCCC;
	padding: 0.25em;
	background-color: #FFFFFF;
	background-image: url(/images/silver.gif); 
	background-repeat: repeat-x;
	color: #333333;
	font-size: 75%;
	font-weight: bold;
	font-family: Verdana, Arial;
}


.button {
	background-color: #4167b4;
	/*background-color: #4f7cc2;*/
	color: white;
	font-weight: bold;
	font-size: x-small;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	padding: 1px 5px;
	border: 1px solid #4164b4;
	display: block;
	text-decoration: none;
}

.button_disabled {
	background-color: #a0a0a0;
	color: white;
	font-weight: bold;
	font-size: x-small;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	padding: 1px 5px;
	border: 1px solid #a0a0a0;
	display: block;
	text-decoration: none;
}

a.button:hover { 
	text-decoration: underline;
}

tr.row_ovr {
	vertical-align: top;
	background-color: #DCEAF2;
	cursor: pointer;
}

#publicationtable {
	background:#d0d0d0;
}

#publicationtable th {
	padding: 5px;
}

#publicationtable td {
	padding: 5px;
}


div.publicationTitle {
	font-weight: bold;
	background: #C0C0C0;
	color: #ffffff;
	padding: 3px 5px;
}

@media print {
	td {
     	font-size: 11px;
	}
	th {
		font-size: 10px;
		font-weight: bold;
	}
	table.hideDuringPrint {
		display: none;
	}
}

.addressOverview {
	padding-top: 2px;
	padding-left: 5px;
	font-weight: bold;
	font-name: verdana, tahoma; 
}

.addressOverviewNormal {
	padding-top: 2px;
	padding-left: 5px;
	font-weight: normal;
	font-name: verdana, tahoma; 
}

a.addressOverview {	
	font-weight: bold;
	color: #0000FF;
}

a.mainMenuItem {
	font-name: verdana, tahoma; 
	font-weight: bold; 
	font-size: x-small;
	color: #000000; 
	text-decoration: none; 
}

a.mainMenuItem:hover {
	color: #253781; 
	cursor: pointer;
}

a.mainMenuItemSelected {
	font-name: verdana, tahoma; 
	font-weight: bold; 
	font-size: x-small;
	color: #ffffff; 
	text-decoration: none; 
}


a.subMenuItem {
	font-name: verdana, tahoma; 
	font-weight: bold; 
	font-size: x-small;
	color: #253781; 
	text-decoration: none; 
}

a.subMenuItem:hover {
	color: #7385b1;
	cursor: pointer;
}

a.subMenuItemSelected {
	font-name: verdana, tahoma; 
	font-weight: bold; 
	font-size: x-small;
	color: #ffffff; 
	background-color: #7385b1; 
	text-decoration: none; 
}

td.mainMenuBorderLeft {
	background-image: url("/images/menuControl/mainMenuBorderLeft.gif");
}

td.mainMenuBackground {
	background-image: url("/images/menuControl/mainMenuBackground.gif");
}

td.mainMenuBorderRight {
	background-image: url("/images/menuControl/mainMenuBorderRight.gif");
}

td.mainMenuBorderLeftSelected {
	background-image: url("/images/menuControl/mainMenuBorderLeftSelected.gif");
}

td.mainMenuBackgroundSelected {
	background-image: url("/images/menuControl/mainMenuBackgroundSelected.gif");
}

td.mainMenuBorderRightSelected {
	background-image: url("/images/menuControl/mainMenuBorderRightSelected.gif");
}

td.subMenuBackground {	
	background-image: url("/images/menuControl/submenuBackground.gif");
}

td.caption {
	font-name: verdana, arial;
	font-size: large;
	font-weight: bold;
	color: #5c74a9;
}

span.textAsButton {
	background-color: #4167b4;
	color: white;
	font-weight: bold;
	font-size: x-small;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	text-decoration: none;
}

table.userInfo {
	margin-top: 10px;
	margin-left: 25px;
	width: 630px;	
}

td.userInfoCenter {
	text-align: center;
}

table.report th, th.sort {
	color: #000000;
	BACKGROUND-COLOR: #D4D0C8;
	font-weight: normal;
	border-top: 1px solid rgb(255,255,255);
	border-left: 1px solid rgb(255,255,255);
	border-bottom: 1px solid rgb(128,128,128);
	border-right: 1px solid rgb(128,128,128);
	padding : 0px 9px 0px 9px;
	text-align: left;
}

table.report td {
	padding-left: 9px;
}

td.sectionBody p {
	height: 1em;
}

td.tableRowHeader {
	font-size: 11px;
	font-weight: bold;
	color: #ffffff;
	background-color: #5C74A9;
	background-image: url(/images/tableHdrbg.gif);
	background-repeat: repeat-x;
	padding: 0px 9px;
	height: 20px;
}

td.tableRowHeader2 {
	font-size: 14px;
	font-weight: bold;
	color: #ffffff;
	background-color: #5C74A9;
	background-image: url(/images/tableHdrbg.gif);
	background-repeat: repeat-x;
	padding: 0px 9px;
	height: 20px;
}

table.report caption {
	text-align:left;
	font-weight:bold;
	color:#5a75ad;
	padding-left:10px;
	height: 14px;
}

table.block {
	width: 100%;
	margin: 10px 0;
	border-collapse: collapse;
}

table.block td, table.block th {
	vertical-align: top;
	border: 1px solid #fff;
	padding: 2px 10px;
}

table.block td {
	background-color: #eee;
}

table.block th {
	background-color: #5C74A9;
	color: #ffffff;
	font-weight: bold;
	text-align : left;
}

#shadow-container {
	position: relative;
	left: 3px;
	top: 3px;
	margin-right: 3px;
	margin-bottom: 3px;
}

#shadow-container .shadow2,
#shadow-container .shadow3,
#shadow-container .container {
	position: relative;
	left: -1px;
	top: -1px;
}

#shadow-container .shadow1 {
	background: #F1F0F1;
}

#shadow-container .shadow2 {
	background: #DBDADB;
}

#shadow-container .shadow3 {
	background: #B8B6B8;
}

#shadow-container .container {
	background: #f5f5f5;
	border: 1px solid #848284;
	padding: 10px 0px 10px 10px;
}
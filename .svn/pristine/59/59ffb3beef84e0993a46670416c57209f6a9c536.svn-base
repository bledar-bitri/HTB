using System.Collections;
using HTB.Database;
using HTB.Database.Views;

namespace HTBExtras.XML
{
    public class XmlInterventionAkt2 : XmlInterventionAkt
    {
        public ArrayList journalList { get; set; }

        public  XmlInterventionAkt2()
        {
            journalList = new ArrayList();
        }

        public void AddJournal(Record journal)
        {
            journalList.Add(journal);
        }
    }
}
